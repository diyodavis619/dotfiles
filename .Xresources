
URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.transparent:          false
URxvt.fading:               0
URxvt.loginShell:           true
URxvt.saveLines:            1000
URxvt.internalBorder:       3
URxvt.lineSpace:            0

! Appearance
URxvt.font:           xft:monospace:size=14:antialias=true
URxvt.imFont:         xft:monospace:im:size=14:antialias=true
URxvt.boldFont:       xft:monospace:bold:size=14:antialias=true
URxvt.italicFont:     xft:monospace:italic:size=14:antialias=true
URxvt.boldItalicFont: xft:monospace:boldItalic:size=14:antialias=true

! Fix font space
!URxvt*letterSpace: -1

! Colored man pages 
URxvt.colorIT:      #87af5f
URxvt.colorBD:      #d7d7d7
URxvt.colorUL:      #87afd7

! Scrollbar
URxvt*scrollBar_right: true
URxvt*scrollBar_floating: true
URxvt*thickness: 8
URxvt.scrollStyle:          plain
URxvt.scrollBar:            true

! Perl extensions
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorColor:          #657b83
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true

!!*fading: 40
*fadeColor: #002b36
*cursorColor: #ffffff
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

! --- special colors ---

*background: #000000
*foreground: #ffffff

